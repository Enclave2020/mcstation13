        $(function () {
            $.getJSON('https://my.nashnet.ua/ajax.php', {getState: 1}, function (data) {
                if (data) {
                    if (data.can_pay_and_activate_montly_tariff) {
                        $('#login_info_text_block1').hide();
                        $('#login_info_text_block2, #can_pay_and_activate_montly_tariff_block').show();
                    } else {
                        $('#error_text').html(data.state);
                        $('#error_block').show();
                    }
                }
            });
        });
    </script>


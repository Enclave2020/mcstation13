	term.clear()
	textutils.slowPrint("doorOS Installer")
	if (fs.exists("startup")) then
		fs.delete("startup")
		textutils.slowPrint("Previous boot file had removed.")
	end
	textutils.slowPrint("Begin coping data...")
	fs.copy("/disk/boot", "startup")
	textutils.slowPrint("Set door access level:")
	a = read()
	settings.set("pid", "5Qj9JBU9")
	settings.set("level", a)
	settings.save("settings")
	textutils.slowPrint("Rebooting")
	disk.eject("top")
	os.reboot()
